<select id="{{ id }}" class="{{class}}" ng-disabled="readOnly" ng-model="ngmodelvar">
    <option ng-repeat="i in ctrl.examSeries" ng-selected="ngmodelvar == i.id" ng-value="{{ i.id }}" value="{{ i.id }}">
        {{ i.examSeries }} - {{ i.examYear }} ({{ i.examBoard.description }})
    </option>
</select>