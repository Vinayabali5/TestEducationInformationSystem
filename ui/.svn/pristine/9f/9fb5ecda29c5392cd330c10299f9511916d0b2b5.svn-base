<table class="table table-hover">
    <thead>
        <th ng-show="showAll || showStudent">Student</th>
        <th>Level</th>
        <th>Qualification Title</th>
        <th>Exam Board</th>
        <th>Grade</th>
    </thead>
    <tbody>
        <tr ng-repeat="studentPredictedGrade in studentPredictedGrades">
            <td ng-show="showAll || showStudent">{{ studentPredictedGrade.studentId }}</td>
            <td>{{ studentPredictedGrade._entryQualification._entryQualificationTypeDescription }}</td>
            <td>{{ studentPredictedGrade._entryQualification.title }}</td>
            <td>{{ studentPredictedGrade._examBoardName }}</td>
            <td>{{ studentPredictedGrade.grade }}</td>
        </tr>
    </tbody>
</table>