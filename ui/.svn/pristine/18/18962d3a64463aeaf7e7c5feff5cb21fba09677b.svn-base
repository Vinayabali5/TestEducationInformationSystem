<form id="entryQualificationTypesEditForm" name="entryQualificationTypesEditForm" role="form" novalidate show-validation>
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true" ng-click="ctrl.cancel()">&times;</button>
        <h4 class="modal-title" id="myToDoLabel">Add/Edit Entry Qualification Type</h4>
    </div>

    <div class="modal-body">
        <div class="row">
            <div class="form-group" ng-show="true">
                <label for="id" class="control-label col-xs-4">Entry Qualification Type ID</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="text" class="form-control" id="id" name="id" readonly ng-model="ctrl.entryQualificationTypes.id">
                </div>
            </div>

            <div class="form-group" ng-show="true">
                <label for="code" class="control-label col-xs-4">Code</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="text" class="form-control" id="code" name="code" ng-model="ctrl.entryQualificationTypes.code">
                </div>
            </div>

            <div class="form-group" ng-show="true">
                <label for="description" class="control-label col-xs-4">Description:</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="text" class="form-control text ui-widget-content ui-corner-all" id="description" code="description" ng-model="ctrl.entryQualificationTypes.description"></input>
                </div>
            </div>

            <div class="form-group" ng-show="true">
                <label for="possibleGradeSetId" class="control-label col-xs-4">Possible Grade Set</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <possible-grade-set-selection id="possibleGradeSetIdId" name="possibleGradeSetIdId" class="form-control" ng-model="ctrl.entryQualificationTypes.possibleGradeSetId"></possible-grade-set-selection>
                </div>
            </div>

            <div class="form-group" ng-show="true">
                <label for="size" class="control-label col-xs-4">Size:</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="text" class="form-control text ui-widget-content ui-corner-all" id="size" code="size" ng-model="ctrl.entryQualificationTypes.size"></input>
                </div>
            </div>

            <div class="form-group" ng-show="true">
                <label for="useOfRoe" class="control-label col-xs-4">Use Of Roe:</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="checkbox" class="form-control text ui-widget-content ui-corner-all" id="useOfRoe" name="useOfRoe" ng-model="ctrl.entryQualificationTypes.useOfRoe"></input>
                </div>
            </div>

        </div>
    </div>

    <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="ctrl.cancel()">
            <span class="glyphicon glyphicon-ban-circle"></span>
            Cancel
        </button>
        <button type="button" class="btn btn-primary" ng-click="ctrl.save()">
            <span class="glyphicon glyphicon-save"></span>
            Save
        </button>
    </div>

</form>