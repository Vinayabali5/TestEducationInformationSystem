<table class="table table-hover" ts-wrapper ts-item-name="Student File">
    <thead>
        <th>File Name</th>
        <th>Category</th>
        <th>Created</th>
        <th>Modified</th>
        <th>Actions</th>
    </thead>
    <tbody>
        <tr ng-repeat="studentFile in studentFiles" ts-repeat>
            <td>{{ studentFile.filename }}</td>
            <td>{{ studentFile.category }}</td>
            <td>{{ studentFile.createdDate | date:'dd/MM/yyyy' }}</td>
            <td>{{ studentFile.modifiedDate | date:'dd/MM/yyyy' }}</td>
            <td>
                <button type="button" class="btn btn-primary btn-sm" ng-click="ctrl.openStudentFiles(studentFile)"><span class="glyphicon glyphicon-save"></span> Download</button>
            </td>
        </tr>
    </tbody>
</table>