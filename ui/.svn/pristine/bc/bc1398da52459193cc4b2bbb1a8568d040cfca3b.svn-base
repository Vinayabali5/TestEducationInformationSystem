<table id="examSeriesTable" class="table">
    <thead>
        <tr>
            <th>Board Identifier</th>
            <th>Board Code</th>
            <th>Board Name</th>
            <th>Description</th>
            <th>Year</th>
            <th>Series</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="examSeries in examSeriesList | filter:ctrl.filterParams" class="ng-cloak" ng-init="ctrl.callGetIndex(examSeries)">
            <td>{{ examSeries.examBoard.boardIdentifier }}</td>
            <td>{{ examSeries.examBoard.boardCode }}</td>
            <td>{{ examSeries.examBoard.name }}</td>
            <td>{{ examSeries.examBoard.description }}</td>
            <td>{{ examSeries.examYear }}</td>
            <td>{{ examSeries.examSeries }}</td>
            <td>
                <ng-transclude></ng-transclude>
            </td>
        </tr>
    </tbody>
</table>