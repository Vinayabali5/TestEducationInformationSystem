<form id="examResultsEditForm" name="examResultsEditForm" role="form" novalidate show-validation>
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true" ng-click="ctrl.cancel()">&times;</button>
        <h4 class="modal-title" id="myToDoLabel">Edit Exam Result - {{ctrl.examResults.studentId}}</h4>
    </div>

    <div class="modal-body">
        <div class="row">
            <div class="form-group" ng-show="true">
                <label for="candidateNo" class="control-label col-xs-4">Candidate No:</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="text" class="form-control" name="id" readonly ng-model="ctrl.examResults.candidateNo">
                </div>
            </div>

            <div class="form-group" ng-show="true">
                <label for="boardIdentifier" class="control-label col-xs-4">Board Identifier:</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="text" class="form-control text ui-widget-content ui-corner-all" id="boardIdentifier" name="boardIdentifier" readonly ng-model="ctrl.examResults.boardIdentifier"></input>
                </div>
            </div>

            <div class="form-group">
                <label for="resultCode" class="control-label col-xs-4">Result Code</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="text" class="form-control text ui-widget-content ui-corner-all" id="resultCode" name="resultCode" readonly ng-model="ctrl.examResults.resultCode"></input>
                </div>
            </div>

            <div class="form-group">
                <label for="examSeries" class="control-label col-xs-4">Exam Series</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="text" class="form-control text ui-widget-content ui-corner-all" id="examSeries" name="examSeries" readonly ng-model="ctrl.examResults.examSeries"></input>
                </div>
            </div>

            <div class="form-group">
                <label for="examYear" class="control-label col-xs-4">Exam Year</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="text" class="form-control text ui-widget-content ui-corner-all" id="examYear" name="examYear" readonly ng-model="ctrl.examResults.examYear"></input>
                </div>
            </div>


            <div class="form-group">
                <label for="score" class="control-label col-xs-4">Score:</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="text" class="form-control text ui-widget-content ui-corner-all" id="score" name="score" ng-model="ctrl.examResults.score"></input>
                </div>
            </div>

            <div class="form-group">
                <label for="grade" class="control-label col-xs-4">Grade:</label>
                <div class="col-xs-8 col-sm-8 col-md-8">
                    <input type="text" class="form-control text ui-widget-content ui-corner-all" id="grade" name="grade" ng-model="ctrl.examResults.grade"></input>
                </div>
            </div>

        </div>
    </div>

    <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="ctrl.cancel()">
            <span class="glyphicon glyphicon-ban-circle"></span>
            Cancel
        </button>
        <button type="button" class="btn btn-primary" ng-click="ctrl.save()">
            <span class="glyphicon glyphicon-save"></span>
            Save
        </button>
    </div>

</form>